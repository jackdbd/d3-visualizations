{
  "name": "d3-visualizations",
  "description": "Visualizations with d3",
  "author": "jackdbd <jackdebidda@gmail.com> (http://www.giacomodebidda.com/)",
  "homepage": "https://jackdbd.github.io/d3-visualizations",
  "repository": "git@github.com:jackdbd/d3-visualizations.git",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "babel": "babel --plugins @babel/plugin-syntax-dynamic-import src/js/index.js",
    "build": "yarn lint && tsc && webpack --config webpack.prod.js --progress",
    "ci": "yarn test && yarn coverage && yarn build",
    "clean": "rimraf build",
    "coverage": "codecov",
    "deploy": "gh-pages -d build",
    "dash": "webpack-dashboard -- webpack-dev-server --config webpack.dev.js",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "format": "prettier --write '{,!(node_modules)/}**/*.{js,jsx,ts,tsx}'",
    "lint": "yarn lint:css && yarn lint:js && yarn lint:ts",
    "lint:css-complexity": "constyble src/css/main.css --config .constyblerc.json",
    "lint:css-style": "stylelint --config .stylelintrc.json --formatter verbose './src/**/*.{css,js,jsx,ts,tsx}'",
    "lint:css": "yarn lint:css-complexity && yarn lint:css-style",
    "lint:js": "eslint src/js --fix",
    "lint:ts": "tslint --config tslint.json --project tsconfig.json",
    "ncu": "ncu",
    "ncuu": "ncu --upgrade",
    "nuke": "rimraf node_modules && rm yarn.lock",
    "predeploy": "yarn clean && yarn build",
    "static": "chromium-browser build/index.html",
    "stats": "webpack-bundle-analyzer build/stats.json --port 8888",
    "test": "jest --verbose",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "updtr": "updtr --use yarn --test-stdout",
    "webhint": "hint https://jackdbd.github.io/d3-visualizations/",
    "webhint:report": "python -m webbrowser ~/repos/d3-visualizations/hint-report/https-jackdbd-github-io-d3-visualizations/index.html"
  },
  "dependencies": {
    "chroma-js": "2.0.6",
    "cross-fetch": "3.0.4",
    "d3": "5.12.0",
    "d3-array": "2.3.1",
    "d3-axis": "1.0.12",
    "d3-color": "1.3.0",
    "d3-dsv": "1.1.1",
    "d3-fetch": "1.1.2",
    "d3-format": "1.4.1",
    "d3-geo": "1.11.6",
    "d3-horizon-chart": "0.0.6",
    "d3-interpolate": "1.3.2",
    "d3-line-chunked": "1.4.1",
    "d3-request": "1.0.6",
    "d3-scale": "3.1.0",
    "d3-scale-chromatic": "1.5.0",
    "d3-selection": "1.4.0",
    "d3-shape": "1.3.5",
    "d3-svg-annotation": "2.5.1",
    "d3-svg-legend": "2.25.6",
    "d3-time": "1.0.11",
    "d3-time-format": "2.1.3",
    "d3-timer": "1.0.9",
    "d3-tip": "0.9.1",
    "d3-transition": "1.2.0",
    "d3-voronoi": "1.1.4",
    "d3-zoom": "1.8.3",
    "fluture": "11.0.3",
    "node-correlation": "1.0.2",
    "paper": "0.12.3",
    "ramda": "0.26.1",
    "sanctuary": "2.0.0",
    "topojson": "3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/runtime": "7.6.0",
    "@hint/configuration-web-recommended": "7.1.0",
    "@packtracker/webpack-plugin": "2.2.0",
    "@types/d3": "5.7.2",
    "@types/d3-array": "2.0.0",
    "@types/d3-axis": "1.0.12",
    "@types/d3-scale": "2.1.1",
    "@types/d3-shape": "1.3.1",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-dynamic-import-webpack": "1.1.0",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "3.0.0",
    "constyble": "1.2.0",
    "css-loader": "3.2.0",
    "duplicate-package-checker-webpack-plugin": "3.0.0",
    "eslint": "6.3.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "extract-css-chunks-webpack-plugin": "4.6.0",
    "extract-text-webpack-plugin": "3.0.2",
    "favicons-webpack-plugin": "1.0.2",
    "file-loader": "4.2.0",
    "gh-pages": "2.1.1",
    "hint": "5.2.2",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "image-webpack-loader": "6.0.0",
    "jest": "24.9.0",
    "jest-dom": "3.1.3",
    "mini-css-extract-plugin": "0.8.0",
    "npm-check-updates": "3.1.22",
    "prettier": "1.18.2",
    "style-loader": "1.0.0",
    "stylelint": "10.1.0",
    "stylelint-config-standard": "18.3.0",
    "terser-webpack-plugin": "2.0.1",
    "ts-loader": "6.1.0",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.6.3",
    "updtr": "^3.1.0",
    "url-loader": "2.1.0",
    "webpack": "4.39.3",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-cli": "3.3.8",
    "webpack-dashboard": "3.2.0",
    "webpack-dev-server": "3.8.0",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-merge": "4.2.2"
  },
  "keywords": [
    "d3",
    "typescript",
    "visualizations",
    "webgl",
    "webpack"
  ]
}
