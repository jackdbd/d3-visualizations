{
  "name": "d3-visualizations",
  "description": "Visualizations with d3",
  "author": "jackdbd <jackdebidda@gmail.com> (http://www.giacomodebidda.com/)",
  "homepage": "https://jackdbd.github.io/d3-visualizations",
  "repository": "git@github.com:jackdbd/d3-visualizations.git",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn lint && yarn format && tsc && webpack --config webpack.prod.js --progress",
    "ci": "yarn test && yarn coverage",
    "clean": "rimraf build",
    "coverage": "codecov",
    "deploy": "gh-pages -d build",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "format": "prettier --write '{,!(node_modules)/}**/*.{js,jsx,ts,tsx}'",
    "lint": "yarn lint:css && yarn lint:js && yarn lint:ts",
    "lint:css": "stylelint --config .stylelintrc.json --formatter verbose './src/**/*.{css,js,jsx,ts,tsx}'",
    "lint:js": "eslint src/js --fix",
    "lint:ts": "tslint --config tslint.json --project tsconfig.json",
    "ncu": "ncu",
    "ncuu": "ncu --upgrade",
    "nuke": "rimraf node_modules && rm yarn.lock",
    "predeploy": "yarn clean && yarn build",
    "test": "jest --verbose",
    "static": "chromium-browser build/index.html"
  },
  "dependencies": {
    "d3": "5.7.0",
    "d3-array": "2.0.3",
    "d3-axis": "1.0.12",
    "d3-color": "1.2.3",
    "d3-fetch": "1.1.2",
    "d3-format": "1.3.2",
    "d3-geo": "1.11.3",
    "d3-horizon-chart": "0.0.6",
    "d3-interpolate": "1.3.2",
    "d3-line-chunked": "1.4.1",
    "d3-request": "1.0.6",
    "d3-scale": "2.1.2",
    "d3-scale-chromatic": "1.3.3",
    "d3-selection": "1.3.2",
    "d3-svg-annotation": "2.3.1",
    "d3-svg-legend": "2.25.6",
    "d3-time": "1.0.10",
    "d3-time-format": "2.1.3",
    "d3-tip": "0.9.1",
    "d3-transition": "1.1.3",
    "d3-zoom": "1.7.3",
    "fluture": "10.3.0",
    "node-correlation": "1.0.2",
    "sanctuary": "0.15.0",
    "topojson": "3.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "7.2.3",
    "@types/d3-array": "1.2.4",
    "@types/jest": "23.3.12",
    "@types/node": "10.12.18",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "1.0.0",
    "compression-webpack-plugin": "2.0.0",
    "css-loader": "2.1.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "extract-css-chunks-webpack-plugin": "3.3.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "3.0.1",
    "gh-pages": "2.0.1",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "image-webpack-loader": "4.6.0",
    "jest": "23.6.0",
    "jest-dom": "3.0.0",
    "mini-css-extract-plugin": "0.5.0",
    "npm-check-updates": "2.15.0",
    "prettier": "1.15.3",
    "style-loader": "0.23.1",
    "stylelint": "9.9.0",
    "stylelint-config-standard": "18.2.0",
    "terser-webpack-plugin": "^1.2.1",
    "tslint": "5.12.1",
    "typescript": "3.2.2",
    "url-loader": "1.1.2",
    "webpack": "4.28.4",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-merge": "4.2.1"
  },
  "keywords": [
    "d3",
    "typescript",
    "visualizations",
    "webgl",
    "webpack"
  ]
}
